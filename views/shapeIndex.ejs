<!DOCTYPE html>
<%- include ('header', {title: name + " sightings"}) %>
  <body>
    <%- include ('navbar')%>
    <div
      class="container mx-auto marginTop"
      style="width: 200px;"
    >
      <h3  class = "header-text" style="text-align: center"><%= name.toUpperCase()%></h3>
      <% shape.forEach((i) => { %>
      <h4><a href="/sighting/<%=i%>" class = "seperated">Sighting <%=i+1%></a></h4>
      <br />
      <% }); %>
    </div>
  </body>
</html>
